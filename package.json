{
  "name": "@nabre/scout-cambusa26",
  "private": false,
  "type": "module",
  "workspaces": [
    "src/micro-frontend/*"
  ],
  "scripts": {
    "start": "npm-run-all --parallel start:* ",
    "start:admin": "npm run start -w @scout-cambusa26/admin ",
    "start:allestimento": "npm run start -w @scout-cambusa26/allestimento ",
    "start:forniture": "npm run start -w @scout-cambusa26/forniture ",
    "start:ordinazioni": "npm run start -w @scout-cambusa26/ordinazioni ",
    "start:smistamento": "npm run start -w @scout-cambusa26/smistamento ",
    "build": "npm-run-all clean build:*",
    "build:admin": "npm run build -w @scout-cambusa26/admin",
    "build:allestimento": "npm run build -w @scout-cambusa26/allestimento",
    "build:forniture": "npm run build -w @scout-cambusa26/forniture",
    "build:ordinazioni": "npm run build -w @scout-cambusa26/ordinazioni",
    "build:smistamento": "npm run build -w @scout-cambusa26/smistamento",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "@types/react-redux": "^7.1.33",
    "axios": "^1.7.3",
    "framer-motion": "^11.3.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.0",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "json-server": "^1.0.0-beta.1",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.7",
    "vite": "^5.3.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}